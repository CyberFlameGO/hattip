{
  "name": "@hattip/bundler-cloudflare-workers",
  "version": "0.0.2",
  "description": "Cloudflare Workers bundler for Hattip",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "author": "Fatih Ayg√ºn <cyco130@gmail.com>",
  "repository": "https://github.com/cyco130/vavite",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@cyco130/eslint-config": "^1.0.0",
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "esbuild": "^0.14.28"
  }
}
