{
  "name": "@hattip/router",
  "version": "0.0.3",
  "description": "Express-like imperative router for Hattip",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "author": "Fatih Ayg√ºn <cyco130@gmail.com>",
  "repository": "https://github.com/cyco130/vavite",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint && pnpm test:unit",
    "test:unit": "vitest",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "peerDependencies": {
    "@hattip/core": "workspace:*"
  },
  "devDependencies": {
    "@hattip/core": "workspace:*",
    "@cyco130/eslint-config": "^1.1.0",
    "eslint": "^8.15.0",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4",
    "vitest": "^0.12.6"
  }
}
